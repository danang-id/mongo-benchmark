{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "removeComments": false,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noImplicitAny": true,
        "lib": [ "es6" ]
    },
    "include": [
        "lib"
    ]
}
